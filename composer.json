{
    "name": "tiagosampaio/skyhub-php",
    "description": "This is the official PHP SDK for integration with SkyHub.",
    "type": "library",
    "require": {
        "symfony/console": "^4.0",
        "guzzlehttp/guzzle": "^6.2@dev",
        "monolog/monolog": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5",
        "squizlabs/php_codesniffer": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "SkyHub\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SkyHubTest\\": "test/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tiago Sampaio",
            "email": "tiago@tiagosampaio.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
